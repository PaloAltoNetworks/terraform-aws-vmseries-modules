<?xml version="1.0"?>
<config version="10.2.0" urldb="paloaltonetworks" detail-version="10.2.3">
  <devices>
    <entry name="localhost.localdomain">
      <template>
        <entry name="asg">
          <settings>
            <default-vsys>vsys1</default-vsys>
          </settings>
          <config>
            <devices>
              <entry name="localhost.localdomain">
                <vsys>
                  <entry name="vsys1">
                    <zone>
                      <entry name="trust">
                        <network>
                          <layer3>
                            <member>ethernet1/1</member>
                          </layer3>
                        </network>
                      </entry>
                      <entry name="untrust">
                        <network>
                          <layer3>
                            <member>ethernet1/2</member>
                          </layer3>
                        </network>
                      </entry>
                      <entry name="inbound">
                        <network>
                          <layer3>
                            <member>ethernet1/1.11</member>
                            <member>ethernet1/1.12</member>
                          </layer3>
                        </network>
                      </entry>
                      <entry name="outbound">
                        <network>
                          <layer3>
                            <member>ethernet1/1.20</member>
                          </layer3>
                        </network>
                      </entry>
                      <entry name="eastwest">
                        <network>
                          <layer3>
                            <member>ethernet1/1.30</member>
                          </layer3>
                        </network>
                      </entry>
                    </zone>
                    <import>
                      <network>
                        <interface>
                          <member>ethernet1/1</member>
                          <member>ethernet1/1.11</member>
                          <member>ethernet1/1.12</member>
                          <member>ethernet1/1.20</member>
                          <member>ethernet1/1.30</member>
                          <member>ethernet1/2</member>
                        </interface>
                      </network>
                    </import>
                  </entry>
                </vsys>
                <network>
                  <virtual-router>
                    <entry name="vr">
                      <ecmp>
                        <algorithm>
                          <ip-modulo/>
                        </algorithm>
                        <enable>no</enable>
                        <symmetric-return>no</symmetric-return>
                        <max-path>4</max-path>
                      </ecmp>
                      <protocol>
                        <bgp>
                          <routing-options>
                            <graceful-restart>
                              <enable>yes</enable>
                            </graceful-restart>
                          </routing-options>
                          <enable>no</enable>
                        </bgp>
                        <rip>
                          <enable>no</enable>
                        </rip>
                        <ospf>
                          <enable>no</enable>
                        </ospf>
                        <ospfv3>
                          <enable>no</enable>
                        </ospfv3>
                      </protocol>
                      <interface>
                        <member>ethernet1/1</member>
                        <member>ethernet1/1.11</member>
                        <member>ethernet1/1.12</member>
                        <member>ethernet1/1.20</member>
                        <member>ethernet1/1.30</member>
                        <member>ethernet1/2</member>
                      </interface>
                      <routing-table>
                        <ip>
                          <static-route>
                            <entry name="app1_az1">
                              <path-monitor>
                                <monitor-destinations>
                                  <entry name="app1_az1">
                                    <enable>yes</enable>
                                    <source>DHCP</source>
                                    <destination>10.100.1.1</destination>
                                    <interval>1</interval>
                                    <count>3</count>
                                  </entry>
                                </monitor-destinations>
                                <enable>yes</enable>
                                <failure-condition>any</failure-condition>
                                <hold-time>2</hold-time>
                              </path-monitor>
                              <nexthop>
                                <ip-address>10.100.1.1</ip-address>
                              </nexthop>
                              <bfd>
                                <profile>None</profile>
                              </bfd>
                              <interface>ethernet1/1</interface>
                              <metric>11</metric>
                              <destination>10.104.0.0/16</destination>
                              <route-table>
                                <unicast/>
                              </route-table>
                            </entry>
                            <entry name="app2_az1">
                              <path-monitor>
                                <monitor-destinations>
                                  <entry name="app2_az1">
                                    <enable>yes</enable>
                                    <source>DHCP</source>
                                    <destination>10.100.1.1</destination>
                                    <interval>1</interval>
                                    <count>3</count>
                                  </entry>
                                </monitor-destinations>
                                <enable>yes</enable>
                                <failure-condition>any</failure-condition>
                                <hold-time>2</hold-time>
                              </path-monitor>
                              <nexthop>
                                <ip-address>10.100.1.1</ip-address>
                              </nexthop>
                              <bfd>
                                <profile>None</profile>
                              </bfd>
                              <interface>ethernet1/1</interface>
                              <metric>11</metric>
                              <destination>10.105.0.0/16</destination>
                              <route-table>
                                <unicast/>
                              </route-table>
                            </entry>
                            <entry name="app1_az2">
                              <path-monitor>
                                <monitor-destinations>
                                  <entry name="app1_az2">
                                    <enable>yes</enable>
                                    <source>DHCP</source>
                                    <destination>10.100.65.1</destination>
                                    <interval>1</interval>
                                    <count>3</count>
                                  </entry>
                                </monitor-destinations>
                                <enable>yes</enable>
                                <failure-condition>any</failure-condition>
                                <hold-time>2</hold-time>
                              </path-monitor>
                              <nexthop>
                                <ip-address>10.100.65.1</ip-address>
                              </nexthop>
                              <bfd>
                                <profile>None</profile>
                              </bfd>
                              <interface>ethernet1/1</interface>
                              <metric>12</metric>
                              <destination>10.104.0.0/16</destination>
                              <route-table>
                                <unicast/>
                              </route-table>
                            </entry>
                            <entry name="app2_az2">
                              <path-monitor>
                                <monitor-destinations>
                                  <entry name="app2_az2">
                                    <enable>yes</enable>
                                    <source>DHCP</source>
                                    <destination>10.100.65.1</destination>
                                    <interval>1</interval>
                                    <count>3</count>
                                  </entry>
                                </monitor-destinations>
                                <enable>yes</enable>
                                <failure-condition>any</failure-condition>
                                <hold-time>2</hold-time>
                              </path-monitor>
                              <nexthop>
                                <ip-address>10.100.65.1</ip-address>
                              </nexthop>
                              <bfd>
                                <profile>None</profile>
                              </bfd>
                              <interface>ethernet1/1</interface>
                              <metric>12</metric>
                              <destination>10.105.0.0/16</destination>
                              <route-table>
                                <unicast/>
                              </route-table>
                            </entry>
                          </static-route>
                        </ip>
                      </routing-table>
                    </entry>
                  </virtual-router>
                  <profiles>
                    <interface-management-profile>
                      <entry name="healtcheck">
                        <http>yes</http>
                        <ping>yes</ping>
                        <permitted-ip>
                          <entry name="10.100.5.0/24"/>
                          <entry name="10.100.69.0/24"/>
                        </permitted-ip>
                      </entry>
                    </interface-management-profile>
                  </profiles>
                  <interface>
                    <ethernet>
                      <entry name="ethernet1/1">
                        <layer3>
                          <ndp-proxy>
                            <enabled>no</enabled>
                          </ndp-proxy>
                          <sdwan-link-settings>
                            <upstream-nat>
                              <enable>no</enable>
                              <static-ip/>
                            </upstream-nat>
                            <enable>no</enable>
                          </sdwan-link-settings>
                          <dhcp-client>
                            <enable>yes</enable>
                            <create-default-route>no</create-default-route>
                          </dhcp-client>
                          <interface-management-profile>healtcheck</interface-management-profile>
                          <lldp>
                            <enable>no</enable>
                          </lldp>
                          <units>
                            <entry name="ethernet1/1.11">
                              <ipv6>
                                <neighbor-discovery>
                                  <router-advertisement>
                                    <enable>no</enable>
                                  </router-advertisement>
                                </neighbor-discovery>
                              </ipv6>
                              <ndp-proxy>
                                <enabled>no</enabled>
                              </ndp-proxy>
                              <sdwan-link-settings>
                                <enable>no</enable>
                              </sdwan-link-settings>
                              <adjust-tcp-mss>
                                <enable>no</enable>
                              </adjust-tcp-mss>
                              <tag>11</tag>
                              <dhcp-client>
                                <create-default-route>no</create-default-route>
                              </dhcp-client>
                            </entry>
                            <entry name="ethernet1/1.12">
                              <ipv6>
                                <neighbor-discovery>
                                  <router-advertisement>
                                    <enable>no</enable>
                                  </router-advertisement>
                                </neighbor-discovery>
                              </ipv6>
                              <ndp-proxy>
                                <enabled>no</enabled>
                              </ndp-proxy>
                              <sdwan-link-settings>
                                <enable>no</enable>
                              </sdwan-link-settings>
                              <adjust-tcp-mss>
                                <enable>no</enable>
                              </adjust-tcp-mss>
                              <tag>12</tag>
                              <dhcp-client>
                                <create-default-route>no</create-default-route>
                              </dhcp-client>
                            </entry>
                            <entry name="ethernet1/1.20">
                              <ipv6>
                                <neighbor-discovery>
                                  <router-advertisement>
                                    <enable>no</enable>
                                  </router-advertisement>
                                </neighbor-discovery>
                              </ipv6>
                              <ndp-proxy>
                                <enabled>no</enabled>
                              </ndp-proxy>
                              <sdwan-link-settings>
                                <enable>no</enable>
                              </sdwan-link-settings>
                              <adjust-tcp-mss>
                                <enable>no</enable>
                              </adjust-tcp-mss>
                              <tag>20</tag>
                              <dhcp-client>
                                <create-default-route>no</create-default-route>
                              </dhcp-client>
                            </entry>
                            <entry name="ethernet1/1.30">
                              <ipv6>
                                <neighbor-discovery>
                                  <router-advertisement>
                                    <enable>no</enable>
                                  </router-advertisement>
                                </neighbor-discovery>
                              </ipv6>
                              <ndp-proxy>
                                <enabled>no</enabled>
                              </ndp-proxy>
                              <sdwan-link-settings>
                                <enable>no</enable>
                              </sdwan-link-settings>
                              <adjust-tcp-mss>
                                <enable>no</enable>
                              </adjust-tcp-mss>
                              <tag>30</tag>
                              <dhcp-client>
                                <create-default-route>no</create-default-route>
                              </dhcp-client>
                            </entry>
                          </units>
                        </layer3>
                      </entry>
                      <entry name="ethernet1/2">
                        <layer3>
                          <ndp-proxy>
                            <enabled>no</enabled>
                          </ndp-proxy>
                          <sdwan-link-settings>
                            <upstream-nat>
                              <enable>no</enable>
                              <static-ip/>
                            </upstream-nat>
                            <enable>no</enable>
                          </sdwan-link-settings>
                          <interface-management-profile>healtcheck</interface-management-profile>
                          <lldp>
                            <enable>no</enable>
                          </lldp>
                          <dhcp-client>
                            <create-default-route>yes</create-default-route>
                          </dhcp-client>
                        </layer3>
                      </entry>
                    </ethernet>
                  </interface>
                </network>
                <deviceconfig>
                  <plugins>
                    <vm_series version="3.0.3">
                      <aws-cloudwatch>
                        <name>asg-vmseries</name>
                        <enabled>yes</enabled>
                        <timeout>1</timeout>
                      </aws-cloudwatch>
                    </vm_series>
                  </plugins>
                </deviceconfig>
              </entry>
            </devices>
            <mgt-config>
              <users/>
            </mgt-config>
          </config>
        </entry>
      </template>
    </entry>
  </devices>
</config>
